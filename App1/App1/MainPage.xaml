<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App1.MainPage"
             xmlns:local="clr-namespace:App1"
             Title="RadioButton Demos">
    <TableView>
        <TableRoot>
            <TableSection>
                <TextCell Text="Dersi Geçme İhtimalim?"
                          Detail="Dersi geçme ihtimalinizi hesaplayınız"
                          Command="{Binding NavigateCommand}"
                          CommandParameter="{x:Type local:Page1}" />
                <TextCell Text="sonuçlar"
                          Detail="sonuçları görebiilirsiniz"
                          Command="{Binding NavigateCommand}"
                          CommandParameter="{x:Type local:Page2}" />
            </TableSection>
        </TableRoot>
    </TableView>

</ContentPage>
